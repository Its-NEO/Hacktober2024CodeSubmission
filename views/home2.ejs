        <h1 id="main-title" class="px-6 py-2 text-5xl font-bold text-black border-b-4 border-green-800">
          <%= data.productName%>
        </h1>
        <a href="/profile">
          <div id="icon-div" class="z-20 w-12 h-12 m-4 bg-white border border-black rounded-full top-5 right-10">
            <img src="/images/profile.png" alt="">
          </div>
        </a>
      </div>
