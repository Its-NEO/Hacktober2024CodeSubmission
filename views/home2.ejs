            <div id="sulfite" class="allergen-item allergen-selected">
              <img src="images/assets/Allergens/sulfite.svg" alt="sulfite image missing">
              <p>Sulfite</p>
            </div>
          </div>
        </div>
        <div class="flex flex-col items-start justify-around gap-6 ingredient-list">
          <p class="text-heading">Ingredient Information</p>
          <div class="flex flex-row flex-wrap gap-12 main-cards-ingredients-list">
