<script>
  const allergen = data.foodAPI && data.foodAPI.alergenAlerts;

  console.log(allergen);

  if (allergen && allergen.length > 0) {
    for (let i = 0; i < allergen.length; i++) {
      const allergenCard = document.querySelector(`#${allergen[i]}`);
      if (allergenCard) {
        allergenCard.classList.add('allergen-selected');
      } else {
        console.warn(`Element with ID ${allergen[i]} not found`);
      }
    }
  } else {
    console.log("No allergen alerts available");
  }
</script>

