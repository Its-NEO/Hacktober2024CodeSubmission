                  <clipPath id="clip0_14_53">
                    <rect x="4" y="4" width="40" height="40" rx="20" fill="white" />
                  </clipPath>
                </defs>
              </svg>
            </button>
          </div>

        </form>
      </div>

      <div>
        <p class="mb-2 text-sm font-bold text-black">Search Results</p>
        <div id="search-results" class="flex flex-col gap-4">
          <div class="result-item">
            <div class="result-name">
              <p class="result-header">Pringles</p>
              <p class="result-subheader">Pepsico</p>
            </div>
            <div class="result-right">
              <div class="result-nova-3">
              </div>
              <!-- <div class="result-nova">
                    <img src="../assets/nova/NOVA_group_2.svg" alt="nova image not found" />
                  </div> -->
              <div class="result-information">
                <p class="result-barcode-id">6001068587209</p>
                <div class="result-info-display">
                  <div class="result-allergen">
                    <img src="images/assets/Allergens/nuts.svg" alt="allergen image not found" />
                  </div>
                  <div class="result-allergen">
                    <img src="images/assets/Allergens/egg.svg" alt="allergen image not found" />
                  </div>
                  <div class="result-score-display">
                    <p class="result-score-cat">H</p>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div id="search-history"></div>
      </div>
    </div>



    <div id="info-page"
